@page 
@model KoperasiTentera.Pages.CreateAccountModel
@{
    ViewData["Title"] = "Create Account";
}

<h2>Create Account</h2>

<form method="post">
    <label>Name:</label>
    <input type="text" asp-for="Account.Name" required />

    <label>IC Number:</label>
    <input type="text" asp-for="Account.ICNumber" required />

    <label>Phone Number:</label>
    <input type="text" asp-for="Account.PhoneNumber" required />

    <label>Email:</label>
    <input type="email" asp-for="Account.Email" required />

    <button type="submit">Next</button> 
</form>
